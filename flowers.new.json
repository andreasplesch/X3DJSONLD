                        "#sourceText":[
"ecmascript:",
"\t\t\tfunction set_translation(value) {",
"\t\t\t\ttranslation = value;",
"\t\t\t}",
"\t\t\tfunction translation_changed() {",
"\t\t\t\treturn translation;",
"\t\t\t}",
"\t\t\tfunction initialize() {",
"\t\t\t    translation = new SFVec3f(0, 0, 0);",
"\t\t\t    velocity = new SFVec3f(",
"\t\t\t    \tMath.random() - 0.5,",
"\t\t\t\tMath.random() - 0.5,",
"\t\t\t\tMath.random() - 0.5);",
"\t\t\t}",
"\t\t\tfunction set_fraction() {",
"\t\t\t    translation = new SFVec3f(",
"\t\t\t    \ttranslation[0] + velocity[0],",
"\t\t\t\ttranslation[1] + velocity[1],",
"\t\t\t\ttranslation[2] + velocity[2]);",
"\t\t\t    for (var j = 0; j <= 2; j++) {",
"\t\t\t\t    if (Math.abs(translation[j]) > 10) {",
"\t\t\t\t\tinitialize();",
"\t\t\t\t    } else {",
"\t\t\t\t\tvelocity[0] += Math.random() * 0.2 - 0.1;",
"\t\t\t\t\tvelocity[1] += Math.random() * 0.2 - 0.1;",
"\t\t\t\t\tvelocity[2] += Math.random() * 0.2 - 0.1;",
"\t\t\t\t    }",
"\t\t\t    }",
"\t\t\t    animate_flowers();",
"\t\t\t}",
"",
"\t\t\tfunction initialize() {",
"\t\t\t     resolution = 100;",
"\t\t\t     updateCoordinates(resolution);",
"\t\t\t     if (typeof coordIndexes == 'undefined' || coordIndexes == null) {",
"\t\t\t\tcoordIndexes = new MFInt32();",
"\t\t\t     }",
"\t\t\t     ci = 0;",
"\t\t\t     for ( i = 0; i < resolution-1; i++) {",
"\t\t\t\tfor ( j = 0; j < resolution-1; j++) {",
"\t\t\t\t     coordIndexes[ci] = i*resolution+j;",
"\t\t\t\t     coordIndexes[ci+1] = i*resolution+j+1;",
"\t\t\t\t     coordIndexes[ci+2] = (i+1)*resolution+j+1;",
"\t\t\t\t     coordIndexes[ci+3] = (i+1)*resolution+j;",
"\t\t\t\t     coordIndexes[ci+4] = -1;",
"\t\t\t\t     ci += 5;",
"\t\t\t\t}",
"\t\t\t    }",
"\t\t\t}",
"",
"\t\t\tfunction updateCoordinates(resolution) {",
"\t\t\t     theta = 0.0;",
"\t\t\t     phi = 0.0;",
"\t\t\t     delta = (2 * 3.141592653) / (resolution-1);",
"\t\t\t     if (typeof coordinates == 'undefined' || coordinates == null) {",
"\t\t\t\tcoordinates = new MFVec3f();",
"\t\t\t     }",
"\t\t\t     for ( i = 0; i < resolution; i++) {",
"\t\t\t\tfor ( j = 0; j < resolution; j++) {",
"\t\t\t\t\trho = a + b * Math.cos(c * theta) * Math.cos(d * phi);",
"\t\t\t\t\tcoordinates[i*resolution+j] = new SFVec3f();",
"\t\t\t\t\tcoordinates[i*resolution+j][0] = rho * Math.cos(phi) * Math.cos(theta);",
"\t\t\t\t\tcoordinates[i*resolution+j][1] = rho * Math.cos(phi) * Math.sin(theta);",
"\t\t\t\t\tcoordinates[i*resolution+j][2] = rho * Math.sin(phi);",
"\t\t\t\t\ttheta += delta;",
"\t\t\t\t}",
"\t\t\t\tphi += delta;",
"\t\t\t     }",
"\t\t\t}",
"",
"\t\t\tfunction animate_flowers(fraction, eventTime) {",
"\t\t\t\tchoice = Math.floor(Math.random() * 4);",
"\t\t\t\tswitch (choice) {",
"\t\t\t\tcase 0:",
"\t\t\t\t\ta += Math.random() * 0.2 - 0.1;",
"\t\t\t\t\tbreak;",
"\t\t\t\tcase 1:",
"\t\t\t\t\tb += Math.random() * 0.2 - 0.1;",
"\t\t\t\t\tbreak;",
"\t\t\t\tcase 2:",
"\t\t\t\t\tc += Math.random() * 2 - 1;",
"\t\t\t\t\tbreak;",
"\t\t\t\tcase 3:",
"\t\t\t\t\td += Math.random() * 2 - 1;",
"\t\t\t\t\tbreak;",
"\t\t\t\t}",
"\t\t\t\tif (a > 1) {",
"\t\t\t\t\ta =  0.5;",
"\t\t\t\t}",
"\t\t\t\tif (b > 1) {",
"\t\t\t\t\tb =  0.5;",
"\t\t\t\t}",
"\t\t\t\tif (c < 1) {",
"\t\t\t\t\tc =  4;",
"\t\t\t\t}",
"\t\t\t\tif (d < 1) {",
"\t\t\t\t\td =  4;",
"\t\t\t\t}",
"\t\t\t\tif (c > 10) {",
"\t\t\t\t\tc = 4;",
"\t\t\t\t}",
"\t\t\t\tif (d > 10) {",
"\t\t\t\t\td = 4;",
"\t\t\t\t}",
"\t\t\t\tresolution = 100;",
"\t\t\t\tupdateCoordinates(resolution);",
"\t\t\t}"
]
                      }
