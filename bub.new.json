"ecmascript:",
"\t\t\tfunction set_translation(value) {",
"\t\t\t\ttranslation = value;",
"\t\t\t}",
"\t\t\tfunction translation_changed() {",
"\t\t\t\treturn translation;",
"\t\t\t}",
"\t\t\tfunction initialize() {",
"\t\t\t    translation = new SFVec3f(0, 0, 0);",
"\t\t\t    velocity = new SFVec3f(",
"\t\t\t    \tMath.random() - 0.5,",
"\t\t\t\tMath.random() - 0.5,",
"\t\t\t\tMath.random() - 0.5);",
"\t\t\t}",
"\t\t\tfunction set_fraction() {",
"\t\t\t    translation = new SFVec3f(",
"\t\t\t    \ttranslation[0] + velocity[0],",
"\t\t\t\ttranslation[1] + velocity[1],",
"\t\t\t\ttranslation[2] + velocity[2]);",
"\t\t\t    for (var j = 0; j <= 2; j++) {",
"\t\t\t\t    if (Math.abs(translation[j]) > 10) {",
"\t\t\t\t\tinitialize();",
"\t\t\t\t    } else {",
"\t\t\t\t\tvelocity[0] += Math.random() * 0.2 - 0.1;",
"\t\t\t\t\tvelocity[1] += Math.random() * 0.2 - 0.1;",
"\t\t\t\t\tvelocity[2] += Math.random() * 0.2 - 0.1;",
"\t\t\t\t    }",
"\t\t\t    }",
"\t\t\t}"
]
