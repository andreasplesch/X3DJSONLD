"",
"",
"ecmascript:",
"",
"function set_a(value) {",
"\ta = value;",
"}",
"",
"function set_b(value) {",
"\tb = value;",
"}",
"",
"function set_c(value) {",
"\tc = value;",
"}",
"",
"function set_d(value) {",
"\td = value;",
"}",
"",
"function set_tdelta(value) {",
"\ttdelta = value;",
"}",
"",
"function set_tdelta(value) {",
"\ttdelta = value;",
"}",
"",
"function set_fraction() {",
"\tvar choice = Math.floor(Math.random() * 4);",
"\tif (choice == 0) {",
"\t\tset_a(a + Math.floor(Math.random() * 2) * 2 - 1);",
"\t} else if (choice == 1) {",
"\t\tset_b(b + Math.floor(Math.random() * 2) * 2 - 1);",
"\t} else if (choice == 2) {",
"\t\tset_c(c + Math.floor(Math.random() * 2) * 2 - 1);",
"\t} else if (choice == 3) {",
"\t\tset_d(d + Math.floor(Math.random() * 2) * 2 - 1);",
"\t}",
"\tset_tdelta(tdelta + 0.5);",
"\tset_pdelta(pdelta + 0.5);",
"\tif (a < 1) {",
"\t\tset_a(10);",
"\t}",
"\tif (b < 1) {",
"\t\tset_b(10);",
"\t}",
"\tif (c < 1) {",
"\t\tset_c(4);",
"\t}",
"\tif (c > 20) {",
"\t\tset_c(4);",
"\t}",
"\tif (d < 1) {",
"\t\tset_d(4);",
"\t}",
"\tif (d > 20) {",
"\t\tset_d(4);",
"\t}",
"\t// console.log(a, b, c, d, tdelta, pdelta);",
"}"
